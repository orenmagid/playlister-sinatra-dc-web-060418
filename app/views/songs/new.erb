<form action="/songs" method="post">
  <label for="song_name">Song Name:</label>
  <input type="text" name="song_name" id="Name">
  <label for="genre">Select Genres for this Song</label>
  <% Genre.all.each do |genre|%>
    <input type="checkbox" name="genre_ids[]" value="<%=genre.id%>" id= "<%=genre.name%>"><%=genre.name%></input>

  <% end %>
  <label for="artist_name">Artist Name:</label>
  <input type="text" name="artist_name" id="Artist Name">
  <input type="submit" id="Create" value="Create">



</form>
